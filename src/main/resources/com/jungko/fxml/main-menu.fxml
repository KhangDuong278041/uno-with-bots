<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="mainPane" alignment="CENTER" hgap="10" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../css/styles.css" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jungko.controller.MainMenuController">
   <children>
      <Button fx:id="toGameBtn" mnemonicParsing="false" onAction="#toGame" prefHeight="50.0" prefWidth="300.0" text="Play" />
      <Button fx:id="toGameSettingsBtn" mnemonicParsing="false" onAction="#toGameSettings" prefHeight="50.0" prefWidth="300.0" text="Settings" GridPane.rowIndex="1" />
      <Button fx:id="toViewCardsBtn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#toViewCards" text="View cards" GridPane.rowIndex="2" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
</GridPane>
